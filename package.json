{
  "name": "materialcalculator",
  "private": true,
  "version": "2.0.0",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "postinstall": "webpack --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\"",
    "semantic-release": "semantic-release pre && semantic-release post && git add package.json && git commit -m 'chore(meta): Version update'",
    "test": "karma start"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "~5.8.29",
    "babel-plugin-typecheck": "^2.0.0",
    "better-npm-run": "^0.0.4",
    "compression": "^1.6.0",
    "concurrently": "^0.1.1",
    "express": "^4.13.3",
    "history": "1.17.0",
    "invariant": "^2.2.0",
    "json-cycle": "^1.0.5",
    "lodash": "^4.3.0",
    "normalize.css": "^4.2.0",
    "pretty-error": "^1.2.0",
    "query-string": "^3.0.0",
    "react": "^15.3.0",
    "react-dom": "^15.0.1",
    "react-ga": "^2.1.1",
    "react-helmet": "^3.1.0",
    "react-inline-css": "^2.0.0",
    "react-onclickoutside": "^5.3.3",
    "react-redux": "^4.0.0",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-split-pane": "^0.1.44",
    "redux": "^3.0.4",
    "redux-async-connect": "^1.0.0-rc4",
    "scroll-behavior": "^0.3.2",
    "serialize-javascript": "^1.1.2",
    "serve-favicon": "^2.3.0",
    "superagent": "^1.4.0",
    "url-loader": "^0.5.7",
    "warning": "^2.1.0",
    "webpack-isomorphic-tools": "^2.2.18"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "~5.8.33",
    "babel-eslint": "^4.1.3",
    "babel-loader": "~5.3.3",
    "babel-plugin-react-transform": "~1.1.1",
    "babel-runtime": "~5.8.29",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.6",
    "css-loader": "^0.23.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^0.8.0",
    "eslint-plugin-react": "^3.5.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.10",
    "last-release-git-tag": "^1.0.1",
    "node-sass": "^3.8.0",
    "piping": "^1.0.0-rc.3",
    "react-a11y": "^0.2.6",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "1.2.6",
    "redux-devtools": "^3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "sass-loader": "^3.1.2",
    "semantic-release": "^4.3.5",
    "sinon": "^1.17.2",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.13.0",
    "timekeeper": "0.0.5",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.12.2",
    "worker-loader": "^0.7.1"
  },
  "release": {
    "getLastRelease": "last-release-git-tag"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BBB/materialcalculator.com.git"
  },
  "engines": {
    "node": "4.2.1"
  }
}